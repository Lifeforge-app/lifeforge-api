    "viewQuery": "WITH languages_map AS (\n  SELECT \n    books_library_entries.id AS entry_id,\n    json_each.value AS lang_id\n  FROM \n    books_library_entries,\n    JSON_EACH(books_library_entries.languages)\n) SELECT\n  books_library_languages.id,\n  books_library_languages.name,\n  books_library_languages.icon,\n  count(languages_map.entry_id) as amount\nFROM books_library_languages\n  LEFT JOIN languages_map ON languages_map.lang_id = books_library_languages.id\nGROUP BY books_library_languages.id"
