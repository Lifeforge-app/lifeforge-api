{
    "compilerOptions": {
        "target": "ESNext",
        "outDir": "./dist",
        "module": "NodeNext",
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
        "strict": true,
        "skipLibCheck": true,
        "moduleResolution": "NodeNext",
        "strictFunctionTypes": true,
        "strictNullChecks": true,
        "allowUnusedLabels": false
    }
}
