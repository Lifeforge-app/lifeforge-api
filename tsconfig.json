{
  "compilerOptions": {
    "target": "ESNext",
    "outDir": "./dist",
    "module": "ESNext",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "moduleResolution": "node",
    "strictFunctionTypes": true,
    "strictNullChecks": true,
    "allowUnusedLabels": false
  }
}
